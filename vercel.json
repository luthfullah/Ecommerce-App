
{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/categories/",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/api/getall",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET"]
      }
    ]
    
  }
  